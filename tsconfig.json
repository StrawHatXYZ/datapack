{
  "compilerOptions": {
    "jsx": "react",
    "outDir": "./dist/",
    "noImplicitAny": false,
    "module": "es6",
    "allowJs": true,
    "moduleResolution": "node",
    "target": "ES6",
    "lib": ["DOM", "ES6", "DOM.Iterable", "ScriptHost", "ES2016.Array.Include"],
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  }
}
